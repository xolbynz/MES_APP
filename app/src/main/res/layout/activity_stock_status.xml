<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1280dp"
    android:layout_height="620dp">

    <LinearLayout
        android:layout_width="1281dp"
        android:layout_height="613dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="12dp"
                android:text="조회 일자"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_startDate"
                android:layout_width="200dp"
                android:layout_height="45sp"
                android:layout_marginStart="32dp"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="4dp"
                android:ems="10"
                android:focusableInTouchMode="false"
                android:inputType="date"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                app:layout_constraintStart_toEndOf="@+id/textView4"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_endDate"
                android:layout_width="212dp"
                android:layout_height="47dp"
                android:layout_marginStart="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="4dp"
                android:ems="10"
                android:focusableInTouchMode="false"
                android:inputType="date"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                app:layout_constraintStart_toEndOf="@+id/textView5"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="22dp"
                android:text="~"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/btn_stock_search"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toEndOf="@+id/edit_endDate"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/search2blue" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="45dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_marginStart="1dp"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="1dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/stockStatus_tv_amt"
                    android:layout_width="100dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="수량"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_unit"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_unit"
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="단위"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_spec"
                    android:layout_width="150dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="규격"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_itemNm"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_itemNm"
                    android:layout_width="230dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="제품명"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_custNm"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_custNm"
                    android:layout_width="190dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="업체명"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_plan"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_plan"
                    android:layout_width="200dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="수주계획"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_lotNo"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_lotNo"
                    android:layout_width="200dp"
                    android:layout_height="45dp"
                    android:layout_marginStart="1dp"
                    android:layout_marginLeft="1dp"
                    android:gravity="center"
                    android:text="Lot_식별표"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/stockStatus_tv_pacDate"
                    android:layout_width="110dp"
                    android:layout_height="45dp"
                    android:gravity="center"
                    android:text="포장일자"
                    android:textColor="@color/mr_cast_meta_default_text_color"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <GridView
                android:id="@+id/grid_stock_status"
                android:layout_width="1279dp"
                android:layout_height="0dp"
                android:layout_marginStart="1dp"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="80dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>