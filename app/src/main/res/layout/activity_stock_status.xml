<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="1281dp"
        android:layout_height="613dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="39dp"
                android:text="조회 일자"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_startDate"
                android:layout_width="205dp"
                android:layout_height="39dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="20dp"
                android:ems="10"
                android:inputType="date"
                android:textSize="10sp"
                app:layout_constraintStart_toEndOf="@+id/textView4"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_endDate"
                android:layout_width="212dp"
                android:layout_height="47dp"
                android:layout_marginStart="252dp"
                android:layout_marginLeft="252dp"
                android:ems="10"
                android:inputType="date"
                android:textSize="10sp"
                app:layout_constraintBottom_toBottomOf="@+id/edit_startDate"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate"
                app:layout_constraintTop_toTopOf="@+id/edit_startDate"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginEnd="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginBottom="24dp"
                android:text="~"
                app:layout_constraintBottom_toBottomOf="@+id/edit_startDate"
                app:layout_constraintEnd_toStartOf="@+id/edit_endDate"
                app:layout_constraintHorizontal_bias="0.372"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="87dp"
                android:layout_height="48dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                app:layout_constraintStart_toEndOf="@+id/edit_endDate"
                app:layout_constraintTop_toTopOf="@+id/edit_endDate"
                app:srcCompat="@drawable/search2blue" />

            <Button
                android:id="@+id/btn_startDate"
                android:layout_width="97dp"
                android:layout_height="37dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="40dp"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="11dp"
                android:text="Button"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView5"
                app:layout_constraintHorizontal_bias="0.13"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableLayout
                android:id="@+id/table_work"
                android:layout_width="1273dp"
                android:layout_height="528dp"
                android:layout_marginStart="1dp"
                android:layout_marginLeft="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#FFFFFF" />

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/design_default_color_background">

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="118dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="1dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginRight="1dp"
                        android:background="@color/cardview_dark_background"
                        android:gravity="left"
                        android:text="포장일자" />

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="140dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="1dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginRight="1dp"
                        android:background="@color/cardview_dark_background"
                        android:gravity="left"
                        android:text="LOT_식별표" />

                    <TextView
                        android:id="@+id/textView20"
                        android:layout_width="131dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="1dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginRight="1dp"
                        android:background="@color/cardview_dark_background"
                        android:gravity="left"
                        android:text="수주계획" />

                    <TextView
                        android:id="@+id/textView21"
                        android:layout_width="258dp"
                        android:layout_height="22dp"
                        android:layout_marginStart="1dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginEnd="1dp"
                        android:layout_marginRight="1dp"
                        android:background="@color/cardview_dark_background"
                        android:gravity="left"
                        android:text="업체명" />

                    <TextView
                        android:id="@+id/textView49"
                        android:layout_width="320dp"
                        android:layout_height="match_parent"
                        android:text="제품명" />

                    <TextView
                        android:id="@+id/textView50"
                        android:layout_width="114dp"
                        android:layout_height="match_parent"
                        android:text="규격" />

                    <TextView
                        android:id="@+id/textView51"
                        android:layout_width="69dp"
                        android:layout_height="match_parent"
                        android:text="단위" />

                    <TextView
                        android:id="@+id/textView52"
                        android:layout_width="112dp"
                        android:layout_height="match_parent"
                        android:text="수량" />
                </TableRow>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#FFFFFF" />
            </TableLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>