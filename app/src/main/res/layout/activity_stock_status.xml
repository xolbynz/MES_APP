<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1281dp"
    android:layout_height="613dp">

    <LinearLayout
        android:layout_width="1281dp"
        android:layout_height="613dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="36dp"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="28dp"
                android:text="조회 일자"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_startDate"
                android:layout_width="205dp"
                android:layout_height="39dp"
                android:layout_marginStart="68dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:inputType="date"
                android:textColor="#FFFFFF"
                android:textSize="10sp"
                android:focusableInTouchMode="false"
                app:layout_constraintStart_toEndOf="@+id/textView4"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/edit_endDate"
                android:layout_width="212dp"
                android:layout_height="47dp"
                android:layout_marginStart="68dp"
                android:layout_marginLeft="68dp"
                android:ems="10"
                android:inputType="date"
                android:textColor="#FFFFFF"
                android:textSize="10sp"
                android:focusableInTouchMode="false"
                app:layout_constraintBottom_toBottomOf="@+id/edit_startDate"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate"
                app:layout_constraintTop_toTopOf="@+id/edit_startDate"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                android:layout_marginTop="22dp"
                android:layout_marginEnd="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginBottom="22dp"
                android:text="~"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/edit_endDate"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/edit_startDate"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/btn_stock_search"
                android:layout_width="87dp"
                android:layout_height="48dp"
                android:layout_marginStart="28dp"
                android:layout_marginLeft="28dp"
                app:layout_constraintStart_toEndOf="@+id/edit_endDate"
                app:layout_constraintTop_toTopOf="@+id/edit_endDate"
                app:srcCompat="@drawable/search2blue" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="1145dp"
                android:layout_height="55dp"
                android:background="#FFFFFF"
                app:layout_constraintEnd_toEndOf="@+id/grid_stock_status"
                app:layout_constraintStart_toStartOf="@+id/grid_stock_status">

                <TextView
                    android:id="@+id/stockStatus_tv_custNm"
                    android:layout_width="190dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginLeft="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="업체명"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_plan"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_plan"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_plan" />

                <TextView
                    android:id="@+id/stockStatus_tv_itemNm"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="제품명"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_custNm"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_pacDate" />

                <TextView
                    android:id="@+id/stockStatus_tv_pacDate"
                    android:layout_width="110dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="포장일자"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/stockStatus_tv_lotNo"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginLeft="4dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="Lot_식별표"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_pacDate" />

                <TextView
                    android:id="@+id/stockStatus_tv_plan"
                    android:layout_width="130dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginLeft="4dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="수주계획"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_lotNo"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_pacDate"
                    app:layout_constraintVertical_bias="0.75" />

                <TextView
                    android:id="@+id/stockStatus_tv_spec"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="규격"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_itemNm"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_itemNm"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_itemNm"
                    app:layout_constraintVertical_bias="0.444" />

                <TextView
                    android:id="@+id/stockStatus_tv_unit"
                    android:layout_width="90dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginLeft="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="단위"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/stockStatus_tv_amt"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginLeft="2dp"
                    android:background="#F8CCCC"
                    android:gravity="center"
                    android:text="수량"
                    android:textColor="@color/mr_cast_meta_black_scrim"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintStart_toEndOf="@+id/stockStatus_tv_unit"
                    app:layout_constraintTop_toTopOf="@+id/stockStatus_tv_spec"
                    app:layout_constraintVertical_bias="0.75" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <GridView
                android:id="@+id/grid_stock_status"
                android:layout_width="1145dp"
                android:layout_height="423dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:numColumns="3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.111"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>